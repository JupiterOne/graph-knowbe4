{
  "name": "@jupiterone/graph-knowbe4",
  "version": "2.0.0",
  "description": "A JupiterOne managed integration for https://www.knowbe4.com.",
  "main": "dist/index.js",
  "repository": "https://github.com/JupiterOne/graph-knowbe4",
  "license": "SEE LICENSE IN LICENSE",
  "files": [
    "LICENSE",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "j1-integration collect",
    "graph": "j1-integration visualize",
    "graph:types": "j1-integration visualize-types",
    "lint": "eslint . --cache --fix --ext .ts,.tsx",
    "format": "prettier --write '**/*.{ts,js,json,css,md,yml}'",
    "type-check": "tsc",
    "test": "jest",
    "test:env": "LOAD_ENV=1 yarn test",
    "test:ci": "yarn lint && yarn type-check && yarn test",
    "build": "tsc -p tsconfig.dist.json --declaration",
    "prepush": "yarn lint && yarn type-check && jest --changedSince master",
    "prepack": "yarn build"
  },
  "dependencies": {
    "@jupiterone/integration-sdk-core": "^6.7.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.groupby": "^4.6.0",
    "lodash.maxby": "^4.6.0",
    "node-fetch": "^2.6.1",
    "request-promise-native": "^1.0.7"
  },
  "devDependencies": {
    "@jupiterone/integration-sdk-core": "^6.7.0",
    "@jupiterone/integration-sdk-dev-tools": "^6.7.0",
    "@jupiterone/integration-sdk-testing": "^6.7.0",
    "@types/bunyan": "^1.8.5",
    "@types/fs-extra": "^5.0.5",
    "@types/gremlin": "^3.4.2",
    "@types/request-promise-native": "^1.0.16",
    "bunyan": "^1.8.12",
    "dotenv": "^7.0.0",
    "fs-extra": "^7.0.1"
  }
